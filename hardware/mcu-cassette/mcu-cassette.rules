
(rules PCB mcu-cassette
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 1338)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.6)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.6)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 254.2 (type default_PWR))
    (clear 203.4 (type default_SIGNAL))
    (clear 50.0 (type smd_smd))
    (clear 254.2 (type smd_PWR))
    (clear 203.4 (type smd_SIGNAL))
    (clear 254.2 (type "kicad_default"_PWR))
    (clear 203.4 (type "kicad_default"_SIGNAL))
    (clear 254.2 (type PWR_PWR))
    (clear 254.2 (type PWR_SIGNAL))
    (clear 203.4 (type SIGNAL_SIGNAL))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (padstack "Via[0-1]_600:300_um"
    (shape
      (circle F.Cu 600.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 600.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_600:300_um" "Via[0-1]_600:300_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_600:300_um-kicad_default" "Via[0-1]_600:300_um" "kicad_default"
  )
  (via 
    "Via[0-1]_800:400_um-PWR" "Via[0-1]_800:400_um" PWR
  )
  (via 
    "Via[0-1]_600:300_um-PWR" "Via[0-1]_600:300_um" PWR
  )
  (via 
    "Via[0-1]_800:400_um-SIGNAL" "Via[0-1]_800:400_um" SIGNAL
  )
  (via 
    "Via[0-1]_600:300_um-SIGNAL" "Via[0-1]_600:300_um" SIGNAL
  )
  (via_rule
    default "Via[0-1]_600:300_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (via_rule
    PWR "Via[0-1]_600:300_um-PWR"
  )
  (via_rule
    SIGNAL "Via[0-1]_600:300_um-SIGNAL"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class PWR
    AREF GND 5V 3V3
    (clearance_class PWR)
    (via_rule PWR)
    (rule
      (width 254.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class SIGNAL
    D1 D0 RST A0 A1 D2 A2 D3
    A3 D4 A4 D5 A5 D6 A6 D7
    A7 D8 D9 D10 D11 D12 D13 "Net-(D1-Pad1)"
    "Net-(D2-Pad1)"
    (clearance_class SIGNAL)
    (via_rule SIGNAL)
    (rule
      (width 203.2)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)